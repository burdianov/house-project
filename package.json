{
  "name": "house-project",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "apollo client:codegen --target typescript --localSchemaFile schema.gql --outputFlat --includes \"{pages,src}/**\" --excludes \"src/generated/**\" src/generated",
    "codegen:watch": "yarn codegen --watch",
    "db:init": "yarn prisma init",
    "db:generate": "yarn prisma generate",
    "db:migrate": "yarn prisma migrate dev --name init"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@prisma/client": "^3.4.1",
    "@reach/combobox": "^0.16.3",
    "apollo-server-micro": "^3.5.0",
    "class-validator": "^0.13.1",
    "cloudinary": "^1.27.1",
    "cloudinary-react": "^1.7.0",
    "deepmerge": "^4.2.2",
    "firebase": "^9.3.0",
    "firebase-admin": "^10.0.0",
    "geolib": "^3.3.3",
    "graphql": "^15.7.2",
    "js-cookie": "^3.0.1",
    "next": "12.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebaseui": "^6.0.0",
    "react-hook-form": "^7.19.1",
    "react-map-gl": "^6.1.17",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "use-debounce": "^7.0.1",
