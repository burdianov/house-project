    "@types/react": "17.0.34",
    "@types/react-map-gl": "^6.1.1",
    "apollo": "^2.33.8",
    "autoprefixer": "^10.4.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.3",
    "postcss": "^8.3.11",
    "prisma": "^3.4.1",
    "tailwindcss": "^2.2.19",
    "typescript": "4.4.4"
  }
